    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/SEPIO_0000389 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/SEPIO_0000389">
        <rdfs:subPropertyOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000136"/>
        <obo:IAO_0000115>A relation that holds between a statement or proposition which can be captured in part or in whole as a directional relationship between two entities, and the property serving as the predicate in this relationship.</obo:IAO_0000115>
