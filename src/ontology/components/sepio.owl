    <!-- http://purl.obolibrary.org/obo/SEPIO_0000376 -->

    <owl:ObjectProperty rdf:about="http://purl.obolibrary.org/obo/SEPIO_0000376">
        <rdfs:domain rdf:resource="http://purl.obolibrary.org/obo/SEPIO_0000149"/>
